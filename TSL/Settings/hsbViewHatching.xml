<?xml version="1.0" encoding="UTF-8"?>
<Hsb_Map>
	<lst nm="Hatch[]">
		<!-- following is the template of a map definition for a hatch -->
		<lst nm="Hatch">
			<!-- flag to indicate whether this map is to be considered or not -->
			<int nm="isActive" vl="1"/>
			<str nm="Name" vl="Hatch1"/>
			<!-- flag to indicate whether the hatch is anisotropic or not -->
			<!-- anisotropic hatch has different definitions with different directions of cuts -->
			<int nm="Anisotropic" vl="1"/>
			<lst nm="Material[]">
				<!-- list of materials where this hatch can be applied -->
				<!-- * indicates it can be applied for every material if no hatch match the material of the entity -->
				<lst nm="Material">
					<str nm="Name" vl="*"/>
				</lst>
			</lst>
			<lst nm="Contour">
				<!-- flag to show or not the contour -->
				<int nm="Contour" vl="1"/>
				<int nm="Color" vl="-2"/>
				<dbl nm="Thickness" ut="L" vl="0"/>
				<!-- flag to supress cross symbol for beams or not -->
				<int nm="SupressBeamCross" vl="0"/>
			</lst>
			<!-- flag to show or not show the solid (filled) hatch -->
			<!-- Hatch can be a combination of a solid hatch and a pattern hatch -->
			<int nm="SolidHatch" vl="1"/>
			<!-- transparency for the solid hatch -->
			<int nm="SolidTransparency" vl="80"/>
			<int nm="SolidColor" vl="-2"/>
			
			<lst nm="Orientation[]">
				<lst nm="Orientation">
					<!-- map of hatch for the normal vector of section cut most aligned with local X vector of the entity-->
					<str nm="Name" vl="X"/>
					<!-- hatch pattern for this direction-->
					<str nm="Pattern" vl="ANSI31"/>
					<!-- color of hatch -2 by entity -1 by layer-->
					<int nm="Color" vl="-2"/>
					<!-- transparency for this pattern-->
					<int nm="Transparency" vl="0"/>
					<!-- pattern rotation-->
					<dbl nm="Angle" ut="L" vl="0"/>
					<dbl nm="Scale" ut="L" vl="0.1"/>
					<dbl nm="ScaleMin" ut="L" vl="10"/>
					<!-- for static dScale0 = dScale*dGlobalScaling where dScale>=dScaleMin-->
					<!-- for static dScale0 = dScale*dGlobalScaling where dScale=dScaleFac * dMin>=dScaleMin-->
					<int nm="Static" vl="0"/>
				</lst>
				<lst nm="Orientation">
					<str nm="Name" vl="Y"/>
					<str nm="Pattern" vl="ANSI31"/>
					<int nm="Color" vl="-2"/>
					<int nm="Transparency" vl="0"/>
					<dbl nm="Angle" ut="L" vl="-45"/>
					<dbl nm="Scale" ut="L" vl="0.1"/>
					<dbl nm="ScaleMin" ut="L" vl="10"/>
					<int nm="Static" vl="0"/>
				</lst>
				<lst nm="Orientation">
					<str nm="Name" vl="Z"/>
					<str nm="Pattern" vl="ANSI31"/>
					<int nm="Color" vl="-2"/>
					<int nm="Transparency" vl="0"/>
					<dbl nm="Angle" ut="L" vl="-45"/>
					<dbl nm="Scale" ut="L" vl="0.1"/>
					<dbl nm="ScaleMin" ut="L" vl="10"/>
					<int nm="Static" vl="0"/>
				</lst>
			</lst>
		</lst>
		<!-- following is map definition for material Wood-->
		<lst nm="Hatch">
			<int nm="isActive" vl="1"/>
			<str nm="Name" vl="Hatch2"/>
			<int nm="Anisotropic" vl="1"/>
			<lst nm="Material[]">
				<lst nm="Material">
					<str nm="Name" vl="Wood"/>
				</lst>
			</lst>
			<lst nm="Contour">
				<!-- flag to show or not the contour -->
				<int nm="Contour" vl="1"/>
				<int nm="Color" vl="-2"/>
				<dbl nm="Thickness" ut="L" vl="0"/>
				<int nm="SupressBeamCross" vl="0"/>
			</lst>
			<int nm="SolidHatch" vl="1"/>
			<int nm="SolidTransparency" vl="10"/>
			<int nm="SolidColor" vl="-2"/>
			<lst nm="Orientation[]">
				<lst nm="Orientation">
					<str nm="Name" vl="X"/>
					<str nm="Pattern" vl="ANSI31"/>
					<int nm="Color" vl="44"/>
					<int nm="Transparency" vl="0"/>
					<dbl nm="Angle" ut="L" vl="0"/>
					<dbl nm="Scale" ut="L" vl="0.1"/>
					<dbl nm="ScaleMin" ut="L" vl="10"/>
					<int nm="Static" vl="0"/>
				</lst>
				<lst nm="Orientation">
					<str nm="Name" vl="Y"/>
					<str nm="Pattern" vl="ANSI31"/>
					<int nm="Color" vl="44"/>
					<int nm="Transparency" vl="0"/>
					<dbl nm="Angle" ut="L" vl="-45"/>
					<dbl nm="Scale" ut="L" vl="0.1"/>
					<dbl nm="ScaleMin" ut="L" vl="10"/>
					<int nm="Static" vl="0"/>
				</lst>
				<lst nm="Orientation">
					<str nm="Name" vl="Z"/>
					<str nm="Pattern" vl="ANSI31"/>
					<int nm="Color" vl="44"/>
					<int nm="Transparency" vl="0"/>
					<dbl nm="Angle" ut="L" vl="-45"/>
					<dbl nm="Scale" ut="L" vl="0.1"/>
					<dbl nm="ScaleMin" ut="L" vl="10"/>
					<int nm="Static" vl="0"/>
				</lst>
			</lst>
		</lst>
		<!-- following is map definition for material Steel-->
		<lst nm="Hatch">
			<int nm="isActive" vl="1"/>
			<str nm="Name" vl="Hatch3"/>
			<int nm="Anisotropic" vl="1"/>
			<lst nm="Material[]">
				<lst nm="Material">
					<str nm="Name" vl="Steel"/>
				</lst>
			</lst>
			<lst nm="Contour">
				<!-- flag to show or not the contour -->
				<int nm="Contour" vl="1"/>
				<int nm="Color" vl="-2"/>
				<dbl nm="Thickness" ut="L" vl="0"/>
				<int nm="SupressBeamCross" vl="0"/>
			</lst>
			<int nm="SolidHatch" vl="1"/>
			<int nm="SolidTransparency" vl="80"/>
			<int nm="SolidColor" vl="-2"/>
			
			<lst nm="Orientation[]">
				<lst nm="Orientation">
					<str nm="Name" vl="X"/>
					<str nm="Pattern" vl="ANSI31"/>
					<int nm="Color" vl="8"/>
					<int nm="Transparency" vl="0"/>
					<dbl nm="Angle" ut="L" vl="0"/>
					<dbl nm="Scale" ut="L" vl="0.1"/>
					<dbl nm="ScaleMin" ut="L" vl="10"/>
					<int nm="Static" vl="0"/>
				</lst>
				<lst nm="Orientation">
					<str nm="Name" vl="Y"/>
					<str nm="Pattern" vl="ANSI31"/>
					<int nm="Color" vl="8"/>
					<int nm="Transparency" vl="0"/>
					<dbl nm="Angle" ut="L" vl="-45"/>
					<dbl nm="Scale" ut="L" vl="0.1"/>
					<dbl nm="ScaleMin" ut="L" vl="10"/>
					<int nm="Static" vl="0"/>
				</lst>
				<lst nm="Orientation">
					<str nm="Name" vl="Z"/>
					<str nm="Pattern" vl="ANSI31"/>
					<int nm="Color" vl="8"/>
					<int nm="Transparency" vl="0"/>
					<dbl nm="Angle" ut="L" vl="-45"/>
					<dbl nm="Scale" ut="L" vl="0.1"/>
					<dbl nm="ScaleMin" ut="L" vl="10"/>
					<int nm="Static" vl="0"/>
				</lst>
			</lst>
		</lst>
	</lst>
	<lst nm="FilterRule[]">
		<lst nm="FilterRule">
			<str nm="Name" vl="Filter1"/>
			<lst nm="Format[]">
				<lst nm="Format">
					<str nm="Format" vl="@(Type)"/>
					<!-- exclude genbeams with type SF Bottom Plate -->
					<str nm="Value" vl="SF Bottom Plate"/>
					<int nm="Operation" vl="0"/>
					<int nm="BeamPackFormat" vl="1"/>
				</lst>
				<lst nm="Format">
					<str nm="Format" vl="@(Type)"/>
					<!-- exclude genbeams with type Rafter -->
					<str nm="Value" vl="Rafter"/>
					<int nm="Operation" vl="0"/>
					<int nm="BeamPackFormat" vl="1"/>
				</lst>
				<lst nm="Format">
					<str nm="Format" vl="@(ColorIndex)"/>
					<!-- include entities with color 5 -->
					<str nm="Value" vl="5"/>
					<int nm="Operation" vl="1"/>
					<int nm="BeamPackFormat" vl="1"/>
				</lst>
				<lst nm="Format">
					<str nm="Format" vl="@(ColorIndex)"/>
					<!-- include entities with color 2 -->
					<str nm="Value" vl="2"/>
					<int nm="Operation" vl="1"/>
					<int nm="BeamPackFormat" vl="1"/>
				</lst>
				<lst nm="Format">
					<str nm="Format" vl="@(EntityType)"/>
					<!-- exclude sheets -->
					<str nm="Value" vl="Sheet"/>
					<int nm="Operation" vl="0"/>
					<int nm="BeamPackFormat" vl="1"/>
				</lst>
			</lst>
		</lst>
		<lst nm="FilterRule">
			<str nm="Name" vl="Filter2"/>
			<lst nm="Format[]">
				<lst nm="Format">
					<str nm="Format" vl="@(EntityType)"/>
					<!-- exclude beams -->
					<str nm="Value" vl="Beam"/>
					<int nm="Operation" vl="0"/>
					<int nm="BeamPackFormat" vl="1"/>
				</lst>
				
			</lst>
		</lst>
	</lst>
	<lst nm="GeneralMapObject">
		<str nm="Identifier" vl="hsbViewHatching"/>
		<str nm="CustomFileName" vl="hsbViewHatching"/>
		<int nm="Version" vl="3"/>
	</lst>
	<unit ut="L" uv="millimeter"/>
	<unit ut="A" uv="radian"/>
</Hsb_Map>
