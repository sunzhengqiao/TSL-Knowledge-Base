<?xml version="1.0" encoding="UTF-8"?>
<Hsb_Map>
	<lst nm="Inventory">
		<!-- If not defined or left blank then General is taken from inventory-->
		<str nm="MinorGroup" vl=""/> 
		<!-- If not defined or left blank then Insulation is taken from inventory-->
		<str nm="MajorGroup" vl=""/> 
	</lst>
	<lst nm="Group">
		<str nm="ObjectName" vl=""/> 
	</lst>
	<lst nm="Pattern[]">
		<lst nm="Pattern">
			<str nm="Name" vl="Default"/>
			<!-- wall codes where this pattern is applied -->
			<!-- this is to have different insulations for different wall types -->
			<lst nm="Wall">
				<str nm="Code" vl="*"/>
				<!-- <str nm="Code" vl="F;A;EA;EB;EC"/> -->
			</lst>
			<!-- flag to controll the creation of sheets -->
			<int nm="CreateSheet" vl="1"/>
			<!-- min allowed insulation dimensions -->
			<dbl nm="MinWidthHeight" ut="L" vl="0"/>
			<!-- -3 not active -->
			<int nm="SheetColor" vl="-3"/>
			<!-- mapping zone assignments  -->
			<lst nm="ZoneMapping[]">
				<!-- create a map for each zone that need be mapped -->
				<lst nm="ZoneMapping">
					<int nm="GeometricZone" vl="0"/>
					<int nm="AssigningZone" vl="0"/>
				</lst>
			</lst>
		</lst>
	</lst>
	<lst nm="GeneralMapObject">
		<str nm="Identifier" vl="hsbElementInsulation"/>
		<str nm="CustomFileName" vl="hsbElementInsulation"/>
		<int nm="Version" vl="1"/>
	</lst>
	<unit ut="L" uv="millimeter"/>
	<unit ut="A" uv="radian"/>
</Hsb_Map>
