<?xml version="1.0" encoding="UTF-8"?>
<Hsb_Map>
	<lst nm="Display">
		<dbl nm="TextHeight" ut="L" vl="50"/>		<!-- optional, the text height of a potential description, default = 50mm -->
		<int nm="Color" vl="40"/>					<!-- optional, the color of the tool symbol,  byIndex = -2, default = 40 -->
		<int nm="Transparency" vl="40"/>			<!-- optional, the transparency of the solid filling of the tool area, default = 0 = no filling -->
		<str nm="DimStyle" vl="Standard"/>			<!-- optional, the dimstyle of a potential description, default = default dimstyle -->
		<lst nm="Extrusion">							<!-- optional list, display parameters for instances with the tool extrusionBody -->
			<dbl nm="TextHeight" ut="L" vl="50"/>		<!-- optional, overrides display.textHeight, the text height of a potential description, default = 50mm -->
			<int nm="Color" vl="40"/>					<!-- optional, overrides display.color, the color of the tool symbol, byIndex = -2, default = 40 -->
			<int nm="ColorRef" vl="150"/>					<!-- optional, overrides display.color, the color of the tool symbol, byIndex = -2, default = 40 -->
			<int nm="Transparency" vl="60"/>			<!-- optional, overrides display.transparency, the transparency of the solid filling of the tool area, default = 0 = no filling -->
			<str nm="DimStyle" vl="Standard"/>			<!-- optional, overrides display.dimStyle, the dimstyle of a potential description, default = default dimstyle -->
		</lst>
		<lst nm="Contour">								<!-- optional list, display parameters for instances with the tool contour -->
			<dbl nm="TextHeight" ut="L" vl="50"/>		<!-- optional, overrides display.textHeight, the text height of a potential description, default = 50mm -->
			<int nm="Color" vl="132"/>					<!-- optional, overrides display.color, the color of the tool symbol, byIndex = -2, default = 40 -->
			<int nm="ColorRef" vl="32"/>					<!-- optional, overrides display.color, the color of the tool symbol, byIndex = -2, default = 40 -->
			<int nm="Transparency" vl="80"/>			<!-- optional, overrides display.transparency, the transparency of the solid filling of the tool area, default = 0 = no filling -->
			<str nm="DimStyle" vl="Standard"/>			<!-- optional, overrides display.dimStyle, the dimstyle of a potential description, default = default dimstyle -->
		</lst>		
	</lst>
	<lst nm="Tool[]">
		<lst nm="Tool">
			<dbl nm="Diameter" ut="L" vl="20"/>		<!-- the tool diameter, must match the diameter of the tool on machine with the given index -->
			<dbl nm="Length" ut="L" vl="170"/>		<!-- optional, not used yet -->
