//region Function TypeOfFastener
	// returns the type of the main fastener component
	String TypeOfFastener(String definition)
	{ 
		String type;
		FastenerAssemblyDef fadef(definition);
		if (fadef.bIsValid())
		{ 
			FastenerListComponent flc = fadef.listComponent();
			FastenerComponentData fcd = flc.componentData();
			type =fcd.type();			
		}	
		
		return type;
	}//endregion
	
//region Function DefinitionsByType
	// returns all or a subset of definitions matching the given type
	// definitions: a filtered set of fastener definitions (i.e. by manufacturer)
	void DefinitionsByType(String type, String& definitions[])
	{ 
		String out[0];
		if (type == "")
		{ 
			return;
		}
		else
		{ 
			for (int i=definitions.length()-1; i>=0 ; i--) 
			{ 
				String definition = definitions[i];
				String typeD = 	FastenerAssemblyDef(definition).listComponent().componentData().type();
				
				//TypeOfFastener(definition);
				if (!Equals(type, typeD))
					definitions.removeAt(i);			
			}//next i			
		}
		return;
	}//endregion

//region Function IsSimpleFastener
	// returns wether the definition describes a simple or an assembly fastener
	// woodscrews are considered to be simple fasteners
	int IsSimpleFastener(String definition)
	{ 
		int bIsSimpleFastener;
		FastenerAssemblyDef fadef(definition);
		if (fadef.bIsValid())
		{ 
			FastenerSimpleComponent fscHeads[] = fadef.headComponents();
			FastenerSimpleComponent fscTails[] = fadef.tailComponents();
