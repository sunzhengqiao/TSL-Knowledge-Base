//region Function Hexagon
	// returns a hexagonal pline defined by the innner circle of a given outer radius
	PLine Hexagon(CoordSys cs, double radius)
	{ 
	// Define inner circle hexagon
		PLine plHex();
		double radiusI = radius / (sqrt(3) * .5);
		for (int i = 0; i < 6; i++)
		{
			double angle= i*60+90;
			Point3d pt (radiusI*cos(angle), radiusI*sin(angle),0);
			plHex.addVertex(pt);
		}
		plHex.close();		
		plHex.transformBy(cs);

		return plHex;
	}//endregion
	
//region Function HexHead
	// returns the body of an hexagonal head
	// int facetMode: 0=do not trim facets, 1=bottom, 2=top, 3=both
	Body HexHead(CoordSys cs, PLine plHex, double diameter, double diameterOuter, double dH, int facetMode)
	{ 
		if (dH<dEps)
		{ 
			//unexpected
			return Body();
		}
	// Create the hexagonal body
		Point3d pt = cs.ptOrg();
		Vector3d vecX = cs.vecX();
		Vector3d vecY = cs.vecY();
		Vector3d vecZ = cs.vecZ();cs.vis(2);
		Body bd(plHex,vecZ*dH,1);
		double dRadiusI = .5*diameterOuter / (sqrt(3) * .5);
		
	// Modify to medium details	

	// subtract inner cylinder
		if (diameter>dEps)
			bd.subPart(Body (pt-vecZ*2*dH, pt+vecZ*2*dH, 0.5*diameter));			//bd.vis(4);		
		
	// subtract tips of hexagon
		{ 
			PLine pl1, pl2;
			pl1.createCircle(pt-vecZ*dEps, vecZ, .5*diameterOuter*1.11);	//pl1.vis(2);
			pl2.createCircle(pt-vecZ*dEps, vecZ, diameterOuter);
			Body bdCyl(pl2, vecZ * 2*dH, 1);
			bdCyl.subPart(Body(pl1, vecZ * 3*dH, 1));						//bdCyl.vis(1);	
			bd.subPart(bdCyl);				
		}
	
	// subtract facets
		if(facetMode>0 && diameterOuter>0)
		{ 
			double dZ = .08 * (diameter>dEps?diameter:.9*diameterOuter);
			bd.vis(6);
			PLine pl(vecY);
			pl.addVertex(pt - vecX * dRadiusI*1.5+vecZ*dZ);
			pl.addVertex(pt - vecX * dRadiusI+vecZ*dZ);
			pl.addVertex(pt - vecX * dRadiusI*.9);
			pl.addVertex(pt - vecX * dRadiusI*.9-vecZ*U(5));
			pl.close();													//pl.vis(3);


			PLine plPath(vecZ); 
			plPath.addVertex(pt - vecX * .45 * diameterOuter);
			plPath.addVertex(pt + vecX * .45 * diameterOuter, tan(45));
			
			// get around accuiracy problems by using a half cone
			//plPath.createCircle(pt, vecZ, .45*diameterOuter);
			//plPath.vis(162);
			Body cone(pl, plPath,32);
			{ 
				CoordSys csMirr; csMirr.setToMirroring(Plane(pt, vecY));
				Body bd = cone;
				bd.transformBy(csMirr);
				cone.addPart(bd);
			}
			
			
			//cone.vis(6);
			if (!cone.isNull())
			{ 
				if (facetMode!=2)
					bd.subPart(cone);
					
				if (facetMode!=1)
				{ 
					CoordSys csMirr; csMirr.setToMirroring(Plane(pt + vecZ * .5 * dH, vecZ));
					cone.transformBy(csMirr);							//cone.vis(6);
					bd.subPart(cone);					
				}				
			}		
		}
	
		return bd;
	}//endregion

//region Function Cylinder
	// returns a cylinder with optional facets
	Body Cylinder(CoordSys cs, double length, double radius, double facet1, double facet2)
	{ 
		Body bd;
		Vector3d vecZ = cs.vecZ();
		Point3d pt1 = cs.ptOrg();
		
	// invalid parameters	
		if (length <= 0 || radius <= 0)
		{
			PLine pl;
			Display dp(1);
			if (length > 0)
				pl=PLine(pt1, pt1 + vecZ * length);
			if (radius > 0)
			{
				pl.createCircle(pt1, vecZ, radius);
			}
			dp.draw(pl);
			return bd;
		}	
		if (facet1 >= .5 * length || facet1<0)facet1 = 0;
		if (facet2 >= .5 * length || facet2<0)facet2 = 0;

		
		Point3d pt2 = pt1;	
		// cone1
		if (facet1>0)
		{
			pt2 += vecZ * facet1;
			bd.addPart(Body(pt1,pt2,radius-facet1,radius));
			pt1 = pt2;
		}
		
		// cylinder
		pt2 = pt1 + vecZ*(length - facet1 - facet2);
		bd.addPart(Body(pt1, pt2, radius));

		// cone2
		if (facet2>0)
		{
			pt1 = pt2;
			pt2 += vecZ * facet2;
			bd.addPart(Body(pt1,pt2,radius,radius-facet2));
		}		
		return bd;
	}//endregion	

//region Function GetComponentBody
	// returns the body of a component
	Body GetComponentBody(FastenerSimpleComponent fsc, CoordSys cs)
	{ 
		FastenerComponentData fcd = fsc.componentData();
		String type = fcd.type();
		double stackThickness = fcd.stackThickness();
		double diameter = fcd.mainDiameter();;
		double diameterOuter = fcd.sinkDiameter();
		String norm = fcd.norm();
		
		Point3d pt = cs.ptOrg();
		Vector3d vecX = cs.vecX();
		Vector3d vecY = cs.vecY();
		Vector3d vecZ = cs.vecZ();
		
		Body bd;
	// HEX
		if (Equals(type,kCTNut))
		{ 
			PLine plHex = Hexagon(cs, .5 * diameterOuter);
			Body bdHex = HexHead(cs,plHex, diameter, diameterOuter, stackThickness, 3);	
			bd= bdHex;//bd.vis(6);
		}
	// Washer
		else if (Equals(type, kCTWasher))
		{
			bd=Body(pt, pt + vecZ * stackThickness, diameterOuter * .5);
			bd.subPart(Body(pt- vecZ*dEps, pt + vecZ * (stackThickness+dEps), diameter * .5));
			
		}
	// LockWasher
		else if (Equals(type, kCTLockWasher))
		{
			bd=Body(pt, pt + vecZ * stackThickness, diameterOuter * .5);
			bd.subPart(Body(pt- vecZ*dEps, pt + vecZ * (stackThickness+dEps), diameter * .5));

			Body sub(pt, vecX, vecY, vecZ, .5*stackThickness, diameterOuter, U(10e3), 0, 1, 0);
			CoordSys rot; rot.setToRotation(atan(stackThickness / (.5*diameter)), vecY, pt + vecZ * .5 * stackThickness);
			sub.transformBy(rot);//sub.vis(6);
			bd.subPart(sub);
		}	
	// SplitRing
		else if (Equals(type, kCTSplitRing))
		{
			bd=Body(pt-vecZ * .5*stackThickness, pt + vecZ * .5*stackThickness, diameterOuter * .5);
			bd.subPart(Body(pt- vecZ*dEps, pt + vecZ * (stackThickness+dEps), diameter * .5));
		}
	// BarrelNut
		else if (Equals(type, kCTBarrelNut))
		{
			bd=Body(pt-vecX*.5*diameterOuter+vecZ * .5*stackThickness, pt +vecX*.5*diameterOuter+ vecZ * .5*stackThickness, .5*stackThickness);
			if (stackThickness>diameter)
				bd.subPart(Body(pt- vecZ*dEps, pt + vecZ * (stackThickness+dEps), diameter * .5));
		}		
	// SquareWasher
		else if (Equals(type, kCTSquareWasher))
		{
			bd=Body(pt, vecX, vecY, vecZ, diameterOuter,diameterOuter,stackThickness,0,0,1);
			bd.subPart(Body(pt- vecZ*dEps, pt + vecZ * (stackThickness+dEps), diameter * .5));

			bd.vis(6);

		}			
		else
			reportMessage("\nGetComponentBody: " + scriptName() + ": Type " + type + " not implemented");
		return bd;
	}//endregio	
	
	
