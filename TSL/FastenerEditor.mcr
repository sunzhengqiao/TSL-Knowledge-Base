//region Function FilterDefinitionsByManufacturer
	// filters the definitions by manufacturer
	void FilterDefinitionsByManufacturer(String manufacturer, String& definitions )
	{ 
		if (Equals(manufacturer, tAnyManufacturer))
			return;
		else
		{ 
			for (int i=definitions.length()-1; i>=0 ; i--) 
			{ 
				FastenerAssemblyDef fadef(definitions[i]);
				if (fadef.bIsValid())
				{ 
					FastenerListComponent flc = fadef.listComponent();
					FastenerComponentData fcd = flc.componentData();
					if (!Equals(manufacturer, fcd.manufacturer()))
					{ 
						definitions.removeAt(i);
						continue;
					}
				}
			}//next i
		}
		return;
	}//endregion

