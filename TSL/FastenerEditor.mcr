//region Function GetDirectoryName
	// returns the directory information for the specified path string, excluding the file name or the last element of the path
	// fullPath: A string containing the path of a file or directory. Can be absolute or relative.
	// path: A string containing the directory part of the path, without the file name. Returns null if path is null. Returns an empty string if path represents a root directory (like "C:\").
	String GetDirectoryName(String fullPath)
	{ 
		String path;
		int x = fullPath.length();

	// find last \	
		int cnt, last=-1;
		while (cnt<x)
		{ 
			int n = fullPath.find("\\", last+1);
			if (n>-1)
				last = n;
			if (n<0)
				break;
			cnt++;
		}
		
		if (last>-1)
			path = fullPath.left(last);
	
		return path;
	}//endregion


		
		
		

