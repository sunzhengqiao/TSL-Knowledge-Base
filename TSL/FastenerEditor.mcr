		//region Function AddIncrementGroup HSB-24043
			// returns
			void AddIncrementGroup(String& list[], String start, String end, double delta, int count, int isLast)
			{ 
		        if (count > 2)
		        {
		            list.append(start + " - " + end);
		        }
		        else
		        {
		            // if less or equal 2 entries show all
		            if (count == 1)
		                list.append(start);
		            else
		                list.append(start + ", " + end);
		        }
		
		        // last group, ensure last value will be shown
		        if (isLast && end != start)
		        {
		        	int n = end.length();
		            // Nur hinzufügen, falls nicht schon am Ende
		            String lastVal = list[list.length() - 1];
		            if (lastVal.right(n)!=end)
		                list.append(end);
		        }
		        
		        Map m; m.setDouble("delta", delta);
		        String postFix = " (+" + this.formatObject("@(delta:PL2)",m) + ")";
		        list[list.length() - 1] += postFix;
		        
				return;
			}//endregion
					
		//region Function GroupEntries HSB-24043
			    String[] GroupEntries(String  entries[])
				{
					String result[0];
					
					if (entries.length() == 0)
						return result;
					
					String start = entries[0];
					String prev = entries[0];
					double delta = 0;
					int count = 1;
					
					// only one entry exists
					if (entries.length() == 1)
					{
						return entries;
					}
					
					delta = entries[1].atof() - entries[0].atof();
					
					for (int i = 1; i < entries.length(); i++)
					{
						int currentDelta = entries[i].atof() - entries[i - 1].atof();
						
						// if delta changes → terminate group
						if (currentDelta != delta)
						{
							AddIncrementGroup(result, start, prev, delta, count, false);
							
							// build new group
							start = entries[i - 1];
							delta = currentDelta;
							count = 1;
						}
						
						prev = entries[i];
						count++;
					}
					
					// Letzte Gruppe hinzufügen
					AddIncrementGroup(result, start, prev, delta, count, true);
					
					return result;
				}//endregion
							
