	String sTriggerImportDwg = T("|Import Styles from Drawing|");
	if (nMode==0)addRecalcTrigger(_kContextRoot, sTriggerImportDwg );	
	if (_bOnRecalc && _kExecuteKey==sTriggerImportDwg)
	{

		String style, sStyles[] = FastenerAssemblyDef().getAllEntryNames();

	// get last used path from default catalog entry
		Map mapProps = this.mapWithPropValuesFromCatalog("FastenerEditor",tDefault);		
		Map propStrings = mapProps.getMap("PropString[]");
		Map mapPath = propStrings.getMap(8);
		String path = mapPath.getString("strValue");

	// Browse for file
		Map mapIn;
		mapIn.setString("Title", T("|Select Drawing with Fastener Styles|"));
		mapIn.setString("StartLocation", path);
		mapIn.setString("FileType", "*.dwg");		
		Map mapOut = callDotNetFunction2(sDialogLibrary, sClass, kBrowseForFile, mapIn);
	
	// Store parent folder for next call
		String filePath= mapOut.getString("FilePath");
		path = GetDirectoryName(filePath);
		if (path.length()>0)
		{
			sLastPath.set(path);
			this.setCatalogFromPropValues(tDefault);
		}
	
	// Import styles
		int bSuccess;
		if (findFile(filePath).length()>0)
		{ 
			
		// Check if selected file is open // HSB-24043	
			if (AcadDatabase().allDwgNames().findNoCase(filePath) >- 1)
			{ 
				Map mpNoticeIn;
				mpNoticeIn.setString("Notice", filePath+TN("\n|The selected file must be closed in AutoCAD to import styles this way.|") + TN("|You can import styles from open files using the Style Manager instead.|"));
				Map mpNoticeOut = callDotNetFunction2(sDialogLibrary, sClass, "ShowNotice", mpNoticeIn);				
			}
		// Import Attempt	
			else
			{
				String importStyles[] = FastenerAssemblyDef().getAllEntryNamesFromDwg(filePath).sorted();
				
			// Alert if no styles found
				if (importStyles.length()<1)
				{ 
					Map mpNoticeIn;
					mpNoticeIn.setString("Notice", T("|No styles are defined in the file located at|\n") + filePath);
					Map mpNoticeOut = callDotNetFunction2(sDialogLibrary, sClass, "ShowNotice", mpNoticeIn);				
				}
			// Styles found
				else
				{ 
					importStyles = ShowSelectStylesDialog(importStyles);
					
					int bHasDuplicate, bOverwrite;
					
				// check potential overwrites
					for (int i=0;i<importStyles.length();i++) 
					{ 
						if (sStyles.findNoCase(importStyles[i],-1)>-1)
						{ 
							bHasDuplicate = true;
							break;
						}				 
					}//next i
					
					if (bHasDuplicate)
					{ 
						Map mpAskIn;
						mpAskIn.setString("Title", T("|Duplicate Style Definitions found|"));
						mpAskIn.setString("question", T("|Overwrite existing styles?|"));
			
						mpAskIn.setString("positiveToolTip", T("|Overwrites the existing style with the new content.|") + TN("|Instances referring to this style will be updated.|"));
						mpAskIn.setString("negativeToolTip", T("|Duplicate style definitions will not be imported.|"));
			
						Map mapAskOut = callDotNetFunction2(sDialogLibrary, sClass, askYesNoMethod, mpAskIn);
						bOverwrite = mapAskOut.getInt("Answer")==1; //__1 for positive, 0 for negative
						//reportNotice("\nwill " + (!bSave?"not ":"")+ " save");	
					}
		
				// Import selected styles
					if (importStyles.length()>0)
					{ 
						String err = FastenerAssemblyDef().importFromDwg(filePath, importStyles, bOverwrite); 			
						if (err.length()>0)
						{
							reportMessage("\n" + importStyles + T(": |import failed|, ") + err);
						}	
						else
						{
							if (bHasDuplicate && !bOverwrite)
								reportMessage("\n" + T("|Import completed, duplicates ignored from| ") + filePath);				
							else
								reportMessage("\n" + importStyles + T(": |successfully imported from| ") + filePath);
							bSuccess = true;
						}	
						
					// Force recalc of existing fasteners
						for (int i=0;i<importStyles.length();i++) 
						{ 
							int numFae = UpdateFastenerAssemblies(importStyles[i]); 
							if (numFae>0)
								reportMessage("\n" + numFae + T("|existing fasteners updated|") + importStyles[i]);
						}//next i
							
					}
				}				
			}


		}
	// update with new styles	
		if (bSuccess)
		{ 
			_Map.setInt("OnStyleImport", true);
			pushCommandOnCommandStack("_HSB_Recalc");
			pushCommandOnCommandStack("(handent \""+this.handle()+"\")");	
			pushCommandOnCommandStack("(Command \"\")");		
		}

		setExecutionLoops(2);
		return;
	}//endregion	

