//region Function TranslateString
	// returns true if a translation was found and modifies the value
	int TranslateString(String& value, String listFrom[], String listTo[])
	{ 
		int bOk;
		if (listFrom.length()!=listTo.length())
		{ 
			reportNotice("\TranslateString: unexpected error");
			return bOk;
		}
		int n = listFrom.findNoCase(value ,- 1);
		if (n>-1)
		{
			value = listTo[n];
			bOk = true;
		}
		
		return bOk;
	}//endregion
//region Function TranslateStrings
	// returns true if a translation was found and modifies the value
	int TranslateStrings(String& values[], String listFrom[], String listTo[])
	{ 
		int bOk;
		if (listFrom.length()!=listTo.length())
		{ 
			reportNotice("\TranslateStrings: unexpected error");
			return bOk;
		}
		for (int i=0;i<values.length();i++) 
		{ 
			int n = listFrom.findNoCase(values[i] ,- 1);
			if (n>-1)
			{
				values[i] = listTo[n];
				bOk = true;
			}		 
		}//next i
		return bOk;
	}//endregion


