//				dp.draw(PLine (tslTails[i].ptOrg(), _PtW));		
//			}			
//		}

		
		return;
	}//endregion

//region Function RepositionOnStack
	// transforms the instance to a stacked location 
	void RepositionOnStack(TslInst comps[], Vector3d vecDir, Point3d ptStack, int bIsHead)
	{ 
		// freeze components during repositioning
		for (int i=0;i<comps.length();i++) 	
			comps[i].setFrozen(true);
		
		for (int i=0;i<comps.length();i++) 
		{ 
			TslInst t = comps[i];
			
			Map m = t.map();	
			Map mapC = m.getMap("Component");
			double stackThickness= mapC.getDouble("StackThickness");

			Vector3d vec = ptStack - t.ptOrg();
			if (!vec.bIsZeroLength())
				t.setPtOrg(ptStack);

			ptStack += vecDir * stackThickness;
			//reportNotice("\n	"+_ThisInst.handle()+" RepositionOnStack i: "+i + " "+t.handle()+" "+mapC.getString(kModel) + " stackThickness:"+stackThickness + mapC.getDouble("height") );//XXX
			
		}//next i
		
		// thaw components after repositioning
		for (int i=0;i<comps.length();i++) 	
		{
			if (!comps[i].bIsValid()){ continue;}
			comps[i].setFrozen(false);	
			comps[i].recalc();
		}

		return;
	}//endregion
		
