//region Function DrawTag
	// draws a tag
	void DrawTag(Point3d ptLoc, String text,Vector3d vecX,Vector3d vecY,  Map params)
	{ 
		String dimStyle = params.getString("dimStyle");
		double textHeight = params.getDouble("textHeight");
		int textColor = params.getInt("textColor");
		int bDrawFilled = params.getInt("drawFilled");
		double xFlag= params.getDouble("xFlag");
		double yFlag = params.getDouble("yFlag");

		Display dpWhite(-1), dpText(142);

		if(textColor>256)
			dpText.trueColor(textColor);
		else
			dpText.color(textColor);

		dpWhite.trueColor(rgb(255, 255, 254));
		dpText.dimStyle(dimStyle);
		dpText.textHeight(textHeight);		
		
		int bDrag = _bOnGripPointDrag && _kExecuteKey == "_Pt0";
		if (  bDrag || vecX.bIsZeroLength() || vecY.bIsZeroLength())
		{
			vecX = vecXView;
			vecY = vecYView;
		}
		Vector3d vecZ = vecX.crossProduct(vecY);;
	
		double dX = dpText.textLengthForStyle(text, dimStyle,textHeight);
		double dY = dpText.textHeightForStyle(text, dimStyle,textHeight);

		dpText.draw(text, ptLoc, vecX, vecY, xFlag, yFlag, bDrag?_kDevice:_kModel);
		
		PLine plBox(vecZ);
		Vector3d vec = .5 * (vecX * dX + vecY * dY);
		plBox.createRectangle(LineSeg(ptLoc-vec, ptLoc+vec), vecX, vecY);
		plBox.offset(-.25*textHeight, true);
	
		int xDir = abs(xFlag) > 0 ? xFlag / abs(xFlag) : 0;
		int yDir = abs(yFlag) > 0 ? yFlag / abs(yFlag) : 0;
		
		plBox.transformBy(.5 * xDir * vecX * dX + .5 * yDir * vecY * dY);
		
		if (bDrawFilled)
			dpWhite.draw(PlaneProfile(plBox), _kDrawFilled, 10);		
		dpText.draw(plBox);			

		return;
	}//endregion



//region Function IsSaved
	// returns true if the tsl components and the tail/head components of the assembly match in content and sequence
	int IsSaved(FastenerAssemblyDef fadef, TslInst tails[], TslInst heads[])
	{ 
		String key1, key2;

		for (int i=0;i<heads.length();i++) 
			key1 += "__" + heads[i].propString(sComponentName);
		for (int i=0;i<tails.length();i++) 
			key1 += "__" + tails[i].propString(sComponentName);		

		FastenerSimpleComponent fscHeads[] = fadef.headComponents();
		FastenerSimpleComponent fscTails[] = fadef.tailComponents();
		String compareKeyComponents;

		for (int i=0;i<fscHeads.length();i++) 
			key2 += "__"+fscHeads[i].componentData().model(); 
		for (int i=0;i<fscTails.length();i++) 
			key2 += "__"+fscTails[i].componentData().model();

		return Equals(key1, key2);
	}//endregion




//region Function FindComponentInDefinition
	// returns true if a valid simpleComponnet could be found in the given style
	int FindComponentInDefinition(FastenerAssemblyDef fadef, FastenerSimpleComponent& fsc, String manufacturer, String component)
	{ 		
		int bOk;
		for (int j=0;j<2;j++) 
		{ 
			FastenerSimpleComponent fscs[] = j==0?fadef.headComponents(): fadef.tailComponents(); 
			for (int i=0;i<fscs.length();i++) 
			{ 
				Map m = SimpleComponentToMap(fscs[i]);
				
			// do not add items of invalid manufacturer	
				if (!Equals(manufacturer, tAnyManufacturer) && !Equals(manufacturer, fscs[i].componentData().manufacturer()))
				{ 
					continue;
				}
				if (Equals(component, fscs[i].componentData().model()))
				{
					fsc=fscs[i];
					bOk = true;
				}
			}//next i
		}//next j			
		
		return bOk;
	}//endregion


	
	

//region Function AppendComponentTypes
	// appends component types which are set within the head or tail components
	void AppendComponentTypes(String manufacturer, String& types[], Map& componentMaps, FastenerAssemblyDef fadef)
	{ 

		for (int j=0;j<2;j++) 
		{ 
			FastenerSimpleComponent fscs[] = j==0?fadef.headComponents(): fadef.tailComponents(); 
			for (int i=0;i<fscs.length();i++) 
			{ 
			// do not add items of invalid manufacturer	
				if (!Equals(manufacturer, tAnyManufacturer) && !Equals(manufacturer, fscs[i].componentData().manufacturer()))
				{ 
					continue;
				}
				String typeI= fscs[i].componentData().type(); 
				if (typeI.length()>0 && types.findNoCase(typeI,-1)<0)
				{
					types.append(typeI);
					componentMaps.appendMap(componentMaps.length(), SimpleComponentToMap(fscs[i]));
				}
			}//next i
		}//next j

		return;
	}//endregion

//region Function UpdateFastenerAssemblies
	// Purpose: Find and transform all valid FastenerAssemblyEnt entities
	// that match a given definition string.
	int UpdateFastenerAssemblies(String sDefinition)
	{
	    // Collect all entities of type FastenerAssemblyEnt from the model space
	    Entity entFaes[] = Group().collectEntities(true, FastenerAssemblyEnt(), _kModelSpace);
	    int num = 0;
	
	    // Iterate through each collected entity
	    for (int i = 0; i < entFaes.length(); ++i)
	    {
	        // Safely cast the entity to FastenerAssemblyEnt
	        FastenerAssemblyEnt fae = (FastenerAssemblyEnt)entFaes[i];
	
	        // Check if entity is valid and matches the given definition
	        if (fae.bIsValid() && Equals(fae.definition(), sDefinition))
	        {
	            // Apply a transformation (currently identity vector — placeholder)
	            fae.transformBy(Vector3d(0, 0, 0));
	
	            // Count the transformed entity
	            num++;
	        }
	    }

	    // Return the number of updated assemblies
	    return num;
	}//endregion



