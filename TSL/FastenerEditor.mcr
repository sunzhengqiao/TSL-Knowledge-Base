//region Function OrderComponents
	// returns ordered lists of head and tail component tsls
	void OrderComponents(Entity ents[], TslInst& tslHeads[], TslInst& tslTails[], Vector3d vecZ, TslInst parent)
	{ 
		tslHeads.setLength(0);
		tslTails.setLength(0);
		
		for (int i=ents.length()-1; i>=0 ; i--) 
		{ 
			Entity& ent  = ents[i]; 
			TslInst t = (TslInst)ent;
			if (!t.bIsValid()){ continue;}
			Map map = t.map();
			int mode = map.getInt("mode");
//			if (map.getEntity("parent")!=parent || mode<1)
//			{ 
//				continue;
//			}
//			else 
			if (mode==1)// head
			{ 
				tslHeads.append(t);
			}
			else if (mode==2)// tail
			{ 
				tslTails.append(t);
			}			
		}
		
	// order heads in dir seen from head
		for (int i=0;i<tslHeads.length();i++) 
			for (int j=0;j<tslHeads.length()-1;j++) 
			{
				double d1 = vecZ.dotProduct(_Pt0 - tslHeads[j].ptOrg());
				double d2 = vecZ.dotProduct(_Pt0 - tslHeads[j+1].ptOrg());
				if (d1<d2)
					tslHeads.swap(j, j + 1);
			}
				
	// order tails in tail dir
		for (int i=0;i<tslTails.length();i++) 
			for (int j=0;j<tslTails.length()-1;j++) 
			{
				double d1 = vecZ.dotProduct(_Pt0 - tslTails[j].ptOrg());
				double d2 = vecZ.dotProduct(_Pt0 - tslTails[j+1].ptOrg());
				if (d1>d2)
					tslTails.swap(j, j + 1);
			}		
	
