//region Function GetFastenerBody
	// returns the body of the fastener
	Body GetFastenerBody(CoordSys cs, FastenerSimpleComponent fsc)
	{ 
		Body bdMain, bdHead;
		
		Point3d pt = cs.ptOrg();
		Vector3d vecX = cs.vecX();
		Vector3d vecY = cs.vecY();
		Vector3d vecZ = cs.vecZ();
			
		Map m = SimpleComponentToMap(fsc);
		
	// ComponentData	
		FastenerComponentData fcd = fsc.componentData();
		String type =fcd.type();	
		double mainDiameter = fcd.mainDiameter();
		
	//ArticleData
		FastenerArticleData fad = fsc.articleData();
		double fastenerLength =fad.fastenerLength();
		double threadLength =fad.threadLength();
		Map map =fad.map();
		
		Map mapCustomFields = ArticleDataToMap(fad).getMap("Map");
		
		
		String headType = m.getString("headtype");
		double headDiameter = m.getDouble("headDiameter");
		double headHeight = m.getDouble("headHeight");
		double pilotDiameter = m.getDouble("pilotDiameter");
		double throughDiameter = m.getDouble("throughDiameter");
		
		double dHeadAngle, dTipLengthAngle = 28;
		
	//Head Height
	// assuming rules of square nut height for all types if not specified
		if (headHeight<=0)
		{
//			if (Equals(headType,kThreadedEnd))
//				headHeight = .5*fastenerLength;			
//			
//			else 
			if (mainDiameter<U(5))		headHeight=U(3.5);
			else if (mainDiameter<=U(6))	headHeight=U(4);
			else if (mainDiameter<=U(8))	headHeight=U(5);
			else if (mainDiameter<=U(10))	headHeight=U(6);
			else if (mainDiameter<=U(12))	headHeight=U(8);
			else if (mainDiameter<=U(16))	headHeight=U(12);
			else 							headHeight=.6*mainDiameter;
			
			if (Equals(headType,kOvalHead))
				headHeight *= .5;
				
				
		}
		

		double radius = headDiameter<=0? .9 * mainDiameter:headDiameter*.5;
		pilotDiameter = pilotDiameter > 0 ? pilotDiameter :.85*mainDiameter;
		double facet= .1*mainDiameter;
		
	// half circle extrusion path for half rounded head shapes
		PLine plPath(vecZ);
		plPath.addVertex(pt+vecX * radius);
		plPath.addVertex(pt-vecX * radius, _PtW+vecY * radius);	

		Point3d ptEnd = pt + vecZ * fastenerLength;


	//region Shaft Body
		if (type==kFTBolt)
		{ 
			double d0 = fastenerLength - facet;
			bdMain = Body(pt, pt + vecZ * d0, .5*mainDiameter);
			bdMain.addPart(Body(pt + vecZ * d0,ptEnd , .5*mainDiameter,.5*(mainDiameter-facet)));
		}
		else if (type==kFTScrew)
		{ 
			double dTipLength = .5*mainDiameter /tan(dTipLengthAngle);
			Point3d pt3 = ptEnd;
			Point3d pt2 = ptEnd-vecZ *dTipLength;
			Point3d pt1 = ptEnd-vecZ *threadLength;
			bdMain = Body(pt2,pt3 , .5*mainDiameter,dEps);
			bdMain.addPart(Body(pt1,pt2 , .5*mainDiameter));
			if (fastenerLength-threadLength>0)
				bdMain.addPart(Body(pt,pt1 , .5*pilotDiameter));
		}
		else
			bdMain = Cylinder(cs, fastenerLength, .5*mainDiameter, facet, facet);		
	//endregion 	

	//region Heads

		//region Flat Head
		if (Equals(headType,kFlatHead))
		{ 
			if (headDiameter<=0)
				headDiameter = 1.9 * mainDiameter;		
			
			double dCylindHeight = .2 * mainDiameter < U(2) ? .2 * mainDiameter : U(2);
			double dConicalHeight = .5*headDiameter;
			if (headHeight<=0)
				headHeight = dCylindHeight+dConicalHeight;
			else
			{ 
				dCylindHeight = .1*headHeight< U(2) ? .1 * dCylindHeight : U(2);
				dConicalHeight = headHeight - dCylindHeight;
			}
	
			Body bdHead (Point3d(0,0,0), Point3d(0,0,dCylindHeight), .5*headDiameter);	
			bdHead.addPart(Body(Point3d(0,0,dCylindHeight),Point3d(0,0,dCylindHeight+dConicalHeight), .5*headDiameter, .5*pilotDiameter));;	
			//bdHead.vis(2);
			bdHead.transformBy(cs);
			bdMain.addPart(bdHead);

		}//endregion
		
		//region HexHead or ExteriorFlange
		else if (Equals(headType, kHexHead) && headDiameter>0 && headHeight>0 ||  Equals(headType,kExteriorFlange))
		{ 	
			PLine plHex = Hexagon(cs, .5*headDiameter);			
			Body bdHead = HexHead(cs,plHex, 0, headDiameter, headHeight, 1);//top
			bdHead.transformBy(-vecZ * headHeight);

		// Exterior Flange
			if (headType==kExteriorFlange)
			{ 
				double flangeHeight = headHeight * .1<U(1)?headHeight * .2:headHeight * .1;
				Body bd (pt, pt- vecZ* flangeHeight, headDiameter*.6);
				bdHead.addPart(bd);
			}
			bdMain.addPart(bdHead);
		}				
		//endregion 
	
		//region Interior Flange Head  (Cylinder)
		else if (headType == kInteriorFlange)
		{ 
			Point3d pt0 = Point3d(0, 0, 0);
			Body bdHead(pt0, Point3d(0,0,headHeight), radius);
			bdHead.transformBy(-vecZ * headHeight);
			double flangeHeight = headHeight * .1<U(1)?headHeight * .2:headHeight * .1;
			Body bd (pt0, pt0 + _ZW *flangeHeight, radius*1.1);//assumed washer thickness
			bdHead.addPart(bd);			
			bdHead.transformBy(cs);
			bdMain.addPart(bdHead);
		}//endregion 		
		
		//region Pan Head
		else if (Equals(headType,kPanHead))
		{ 
		// add tolerance cylinder: avoid extruding along a path where the end segment is an arc
			double tol = dEps;
			//bdMain.addPart(Body(_PtW, _PtW + _ZW * headHeight, tol));			

			Body bdHead(_PtW,_PtW-_ZW*.5*headHeight, 	.5*headDiameter);
			bdHead.addPart(Body(_PtW, _PtW - _ZW * headHeight, tol));	
			bdHead.transformBy(cs);
			bdMain.addPart(bdHead);
	
			PLine plShape(_YW);
			plShape.addVertex(_PtW+_XW * tol);
			plShape.addVertex(_PtW+_XW * .5*headDiameter);
			plShape.addVertex(_PtW+_XW * tol-_ZW * .5*headHeight, tan(11.5));
			//plShape.addVertex(_PtW+_XW * tol+_ZW * .5*headHeight);
			plShape.transformBy(-_ZW * .5 * headHeight);
			//plShape.vis(1);
	
			bdHead = Body(plShape, plPath, 16);
			bdHead.transformBy(cs);
			bdMain.addPart(bdHead);
	
		// copy mirrored	
			CoordSys rot;rot.setToRotation(180, vecZ, cs.ptOrg());
			bdHead.transformBy(rot);
			bdMain.addPart(bdHead);

			//bdMain.vis(6);
		}//endregion		
		
		//region Socket Head  (Cylinder)
		else if (Equals(headType,kSocketHead))
		{ 
			Body bdHead (Point3d(0,0,0), Point3d(0,0,headHeight), .5*headDiameter);
			bdHead.transformBy(cs);			//bdHead.vis(2);
			bdMain.addPart(bdHead);
		}//endregion 	
		
		//region Washer Head  (Cylinder)
		else if (Equals(headType,kWasherHead))
		{ 
			Body bdHead (Point3d(0,0,0), Point3d(0,0,headHeight), .5*headDiameter);
			
			
			
		// innerhead
			double headHeightN = mapCustomFields.getDouble("HeadHeightN");
			double coneHeight = .5 * (throughDiameter - pilotDiameter);
			if (throughDiameter>0 && headHeightN>0 && coneHeight>0)
			{ 
				Body bd (Point3d(0,0,headHeight), Point3d(0,0,headHeight+headHeightN), .5*throughDiameter);
				bdHead.addPart(bd);
				bd =Body(Point3d(0,0,headHeight+headHeightN), Point3d(0,0,headHeight+headHeightN+coneHeight),  .5*throughDiameter,.5*pilotDiameter);
				bdHead.addPart(bd);
				
			}
			bdHead.transformBy(cs);			//bdHead.vis(2);
			bdMain.addPart(bdHead);
			
			
			
			
			
			
		}//endregion 		
		
		//region ThreadedRod  (Cylinder)
		else if (type==kFTThreadedRod)//(Equals(headType, kThreadedEnd))
		{
			int g = 66;
		}
		
		else
			reportMessage("nGetFastenerBody: " + scriptName() + " Head Type " + headType + " not implemented");		
	//endregion 	


		return bdMain;
	}//endregion	
			
