//region Function FastenerTypes
	// returns the types found for the given manufacturer
	String[] FastenerTypes(String manufacturer)
	{ 
		String types[0];
		int maxNum = kFTypes.length();
		
		Map mpIn;
		if (manufacturer!=tAnyManufacturer)
			mpIn.setString(kManufacturer, manufacturer);	
	
		//HSB-24011 fastener types are cached, graphics on drag improved
		Map mapRet;
		Map mapCache = _Map.getMap("FastenerTypeCache");
		if (Equals(mapCache.getString("Manufacturer"),manufacturer))
		{ 
			types = GetStringArray(mapCache.getMap("FastenerTypes"), false);			
		}	
		if (_bOnRecalc || types.length()<1)
		{
			mapRet = callDotNetFunction2(kFastenerLib, kFastenerClass, kGetScrewListWithFilter, mpIn);	
		}
		
		for (int i=0;i<mapRet.length();i++) 
		{ 
			Map m = mapRet.getMap(i);
			String type = m.getString(kType);
			
		// Exclude dowels from fastenerAssembly composition	
			if (Equals(type, kFTDowel))
			{ 
				continue;
			}
			
			if (type.length()>0 && types.findNoCase(type,-1)<0)
				types.append(type);
			
			if (types.length()==maxNum) // the list could be very long if manufactuerer is set to any
			{
				break;
			}
			
		}//next i
		
		mapCache.setString("Manufacturer", manufacturer);
		Map m = SetStringArray(types, "");
		mapCache.setMap("FastenerTypes", m);
		_Map.setMap("FastenerTypeCache", mapCache);
		
		return types;
	}//endregion


//region Function ComponentTypes
	// returns the types found for the given manufacturer and family. if the min/maxDiameter is given it will only accept any item matching hte criteria
	String[] ComponentTypes(String manufacturer, double minDiameter, double maxDiameter)
	{ 
		String types[0];
		
		Map mpIn;
		if(manufacturer!=tAnyManufacturer)
			mpIn.setString(kManufacturer, manufacturer);	
		if (maxDiameter>0 && minDiameter==maxDiameter)
		{ 
			mpIn.setDouble("Diameter", maxDiameter);			
		}
		else if (maxDiameter>0)
		{ 
			mpIn.setDouble("MinDiameter", minDiameter);
			mpIn.setDouble("MaxDiameter", maxDiameter);			
		}

		
		Map mapRet;
		mapRet = callDotNetFunction2(kFastenerLib, kFastenerClass, kGetComponents, mpIn);	
		for (int i=0;i<mapRet.length();i++) 
		{ 
			Map m = mapRet.getMap(i);
			String type = m.getString(kType);
			if (type.length()>0 && types.findNoCase(type,-1)<0)
				types.append(type);
			 
		}//next i			
		return types;
	}//endregion

//region Function GetComponents
	// returns the components by model if found for the given manufacturer. if the min/maxDiameter is given it will only accept any item matching hte criteria
	String[] GetComponents(String manufacturer, String type, double minDiameter, double maxDiameter)
	{ 
		String components[0];
		
		Map mpIn;
		if (manufacturer!=tAnyManufacturer)
			mpIn.setString(kManufacturer, manufacturer);
		mpIn.setString(kType, type);
		if (maxDiameter>0 && minDiameter==maxDiameter)
		{ 
			mpIn.setDouble("Diameter", maxDiameter);			
		}
		else if (maxDiameter>0)
		{ 
			mpIn.setDouble("MinDiameter", minDiameter);
			mpIn.setDouble("MaxDiameter", maxDiameter);			
		}

		
		Map mapRet;
		mapRet = callDotNetFunction2(kFastenerLib, kFastenerClass, kGetComponents, mpIn);	
		for (int i=0;i<mapRet.length();i++) 
		{ 
			Map m = mapRet.getMap(i);
			String model = m.getString(kModel);
			if (model.length()>0 && components.findNoCase(model,-1)<0)
				components.append(model);
			 
		}//next i			
		return components;
	}//endregion
			
