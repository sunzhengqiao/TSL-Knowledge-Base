	// returns an ordered list of articleDatas
	void OrderArticleDataSet(FastenerArticleData& fads[], int bAscending)
	{ 
	// order ascending/descending
		for (int i=0;i<fads.length();i++) 
			for (int j=0;j<fads.length()-1;j++) 
				if ((bAscending && fads[j].fastenerLength()>fads[j+1].fastenerLength()) || 
					(!bAscending && fads[j].fastenerLength()<fads[j+1].fastenerLength()))
					fads.swap(j, j + 1);
		return;
	}//endregion

